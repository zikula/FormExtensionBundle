{
    "name": "zikula/formextension-bundle",
    "type": "symfony-bundle",
    "description": "An bundle providing zikula-related symfony form extensions.",
    "keywords": [ "zikula" ],
    "homepage": "https://ziku.la",
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Heydenburg",
            "email": "info@ziku.la"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/contracts": "5.*",
        "symfony/form": "5.*",
        "symfony/framework-bundle": "5.*",
        "symfony/string": "5.*",
        "symfony/validator": "5.*",

        "zikula/core-bundle": "dev-master",
        "zikula/settings-module": "dev-master",
        "zikula/theme-module": "dev-master"
    },
    "autoload": {
        "psr-4": { "Zikula\\Bundle\\FormExtensionBundle\\": "" }
    }
}
